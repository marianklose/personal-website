$PROBLEM    1cmt_iv_sim
$INPUT      ID TIME EVID AMT RATE DV MDV
$DATA      input_data.csv IGNORE=@
$SUBROUTINE ADVAN1 TRANS2
$PK
; define fixed effects parameters
CL = THETA(1) * EXP(ETA(1))
V = THETA(2)

; scaling
S1=V

$THETA  (0,0.2,1) ; 1 TVCL
 3.15 FIX ; 2 TVV
$OMEGA  0.2  ;    1 OM_CL
$SIGMA  0.1  FIX  ;  1 SIG_ADD
$ERROR 
; add additive error
Y = F + EPS(1)

; store error for table output
ERR1 = EPS(1)

; $ESTIMATION METHOD=0 MAXEVAL=9999 SIGDIG=4 PRINT=1 NOABORT
$SIMULATION (12345678) ONLYSIM
$TABLE      ID TIME EVID AMT RATE DV MDV ETA1 ERR1 NOAPPEND ONEHEADER
            NOPRINT FILE=sim_out

  
